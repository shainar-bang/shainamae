const firstFavNum = 6;
const secondFavNum = 9;
//Additon
const sum = firstFavNum + secondFavNum;
console.log(sum);
//Subtraction
const difference = firstFavNum - secondFavNum;
console.log(diffence);
//Multiplication
const product = firstFavNum * secondFavNum;
console.log(product);
//Division
const qoutient = firstFavNum / secondFavNum;
console.log(qoutient);
//Modulus 
const remainder = firstFavNum % secondFavNum;
console.log(remainder);
//Exponentiation(Power)
const power = firstFavNum ** secondFavNum;
console.log(power);




